# .env.template
# Copy this file to .env and fill in your actual values

# Base domain for all services
# Example: onestack.yourdomain.com
BASE_DOMAIN=your-domain.com

# Let's Encrypt Email
# This email will be used for SSL certificate registration and renewal notifications
ACME_EMAIL=your-email@example.com

# Shared Docker network name
# This network will be used by all services to communicate
NETWORK_NAME=web
