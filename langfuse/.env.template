# onestack/langfuse/.env.template
# Copy this file to .env and fill in your actual values

# -- Network Configuration --
BASE_DOMAIN=example.com
WEB_NETWORK_NAME=web
INTERNAL_NETWORK_NAME=internal

# -- Langfuse Database Configuration --
LANGFUSE_DATABASE_URL=postgres://postgres:yourpassword@pgvectordb:5432/langfuse
LANGFUSE_SALT=
LANGFUSE_ENCRYPTION_KEY= # generate via `openssl rand -hex 32`
LANGFUSE_TELEMETRY_ENABLED=false
LANGFUSE_ENABLE_EXPERIMENTAL_FEATURES=true

# -- Clickhouse Configuration --
LANGFUSE_CLICKHOUSE_MIGRATION_URL=clickhouse://clickhouse:9000
LANGFUSE_CLICKHOUSE_URL=http://clickhouse:8123
LANGFUSE_CLICKHOUSE_USER=
LANGFUSE_CLICKHOUSE_PASSWORD=
LANGFUSE_CLICKHOUSE_CLUSTER_ENABLED=false

# -- S3 Event Upload Configuration --
LANGFUSE_S3_EVENT_UPLOAD_BUCKET=langfuse
LANGFUSE_S3_EVENT_UPLOAD_REGION=auto
LANGFUSE_S3_EVENT_UPLOAD_ACCESS_KEY_ID=
LANGFUSE_S3_EVENT_UPLOAD_SECRET_ACCESS_KEY=
LANGFUSE_S3_EVENT_UPLOAD_ENDPOINT=
LANGFUSE_S3_EVENT_UPLOAD_FORCE_PATH_STYLE=true
LANGFUSE_S3_EVENT_UPLOAD_PREFIX=events/

# -- S3 Media Upload Configuration --
LANGFUSE_S3_MEDIA_UPLOAD_BUCKET=langfuse
LANGFUSE_S3_MEDIA_UPLOAD_REGION=auto
LANGFUSE_S3_MEDIA_UPLOAD_ACCESS_KEY_ID=
LANGFUSE_S3_MEDIA_UPLOAD_SECRET_ACCESS_KEY=
LANGFUSE_S3_MEDIA_UPLOAD_ENDPOINT=
LANGFUSE_S3_MEDIA_UPLOAD_FORCE_PATH_STYLE=true
LANGFUSE_S3_MEDIA_UPLOAD_PREFIX=media/

# -- Queue and Performance Configuration --
LANGFUSE_INGESTION_QUEUE_DELAY_MS=
LANGFUSE_INGESTION_CLICKHOUSE_WRITE_INTERVAL_MS=

# -- Redis Configuration --
LANGFUSE_REDIS_HOST=redis
LANGFUSE_REDIS_PORT=6379
LANGFUSE_REDIS_AUTH=

# -- Authentication and Web Configuration --
LANGFUSE_NEXTAUTH_URL=
LANGFUSE_NEXTAUTH_SECRET=

# -- Initial Setup (Optional) --
# LANGFUSE_INIT_ORG_ID=
# LANGFUSE_INIT_ORG_NAME=
# LANGFUSE_INIT_PROJECT_ID=
# LANGFUSE_INIT_PROJECT_NAME=
# LANGFUSE_INIT_PROJECT_PUBLIC_KEY=
# LANGFUSE_INIT_PROJECT_SECRET_KEY=
# LANGFUSE_INIT_USER_EMAIL=
# LANGFUSE_INIT_USER_NAME=
# LANGFUSE_INIT_USER_PASSWORD=

# -- Feature Flags --
LANGFUSE_SDK_CI_SYNC_PROCESSING_ENABLED=false
LANGFUSE_READ_FROM_POSTGRES_ONLY=false
LANGFUSE_READ_FROM_CLICKHOUSE_ONLY=true
LANGFUSE_RETURN_FROM_CLICKHOUSE=true
LANGFUSE_AUTH_DISABLE_SIGNUP=true

