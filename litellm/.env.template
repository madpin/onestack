# onestack/litellm/.env.template
# Copy this file to .env and fill in your actual values

# -- Configuration --
# TODO: Add your configuration variables here
# Example:

POSTGRES_LITELLM_DB=litellm

# ===========================================
# AI/LLM API KEYS
# ===========================================

# OpenAI
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_API_KEY_FREE=your-openai-free-api-key-here
# OPENAI_API_HOST=https://your-openai-host.com/v1

# Anthropic
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Google AI
GEMINI_API_KEY=your-gemini-api-key-here
GEMINI_API_KEY_FREE=your-gemini-free-api-key-here
GOOGLE_APPLICATION_CREDENTIALS=/tmp/google-credentials.json

# Other AI Services
COHERE_API_KEY=your-cohere-api-key-here
DEEPINFRA_API_KEY=your-deepinfra-api-key-here
GROQ_API_KEY=your-groq-api-key-here
HUGGINGFACE_API_KEY=your-huggingface-api-key-here
OPENROUTER_API_KEY=your-openrouter-api-key-here
PERPLEXITYAI_API_KEY=your-perplexity-api-key-here
DEEPSEEK_API_KEY=your-deepseek-api-key-here
HYPERBOLIC_API_KEY=your-hyperbolic-api-key-here
GROK_API_KEY=your-grok-api-key-here
ALICLOUD_API_KEY=your-alicloud-api-key-here
VOYAGE_API_KEY=your-voyage-api-key-here

# Cloud Services
NEBIUS_API_KEY="your-nebius-api-key-here"

# Azure AI
AZURE_AI_API_BASE=https://your-azure-ai-hub.services.ai.azure.com
AZURE_AI_API_KEY=your-azure-ai-api-key-here

# GitHub
GITHUB_TOKEN=your-github-token-here

# ===========================================
# MONITORING & OBSERVABILITY
# ===========================================

# DataDog
DD_API_KEY="your-datadog-api-key-here"
DD_SITE="datadoghq.eu"
DD_LLMOBS_AGENTLESS_ENABLED=1
DD_LLMOBS_ENABLED=1
DD_LLMOBS_ML_APP=litellm
DD_SOURCE=litellm

# Langfuse
LANGFUSE=True
LANGFUSE_HOST=https://your-langfuse-host.com
LANGFUSE_PUBLIC_KEY=your-langfuse-public-key-here
LANGFUSE_SECRET_KEY=your-langfuse-secret-key-here

# LogFire
LOGFIRE_TOKEN=your-logfire-token-here

# PostHog
POSTHOG_API_KEY=your-posthog-api-key-here
POSTHOG_API_URL=https://eu.posthog.com

# Brave Search
BRAVE_API_KEY=your-brave-api-key-here

# ===========================================
# AUTHENTICATION & SECURITY
# ===========================================

# UI Authentication
UI_USERNAME=your-username
UI_PASSWORD=your-secure-password

# LiteLLM Master Key
LITELLM_MASTER_KEY="your-litellm-master-key-here"
LITELLM_SALT_KEY="your-litellm-salt-key-here"

# ===========================================
# APPLICATION CONFIGURATION
# ===========================================

# Application Settings
PWD=/app
STORE_MODEL_IN_DB=True

# Uncomment for debug logging
# LITELLM_LOG=DEBUG

# ===========================================
# COMMENTED OUT / ALTERNATIVE CONFIGURATIONS
# ===========================================

# Alternative Langfuse (Cloud)
# LANGFUSE_HOST=https://cloud.langfuse.com
# LANGFUSE_PUBLIC_KEY=your-cloud-langfuse-public-key
# LANGFUSE_SECRET_KEY=your-cloud-langfuse-secret-key

# Alternative Redis Configuration
# REDIS_HOST=redis
# REDIS_PORT=6379
# REDIS_PASSWORD=your-redis-password

# Domain and URL Configuration
# BASE_URL=https://your-domain.com/v1
# DOMAIN=your-domain.com
# BACKEND_ROOT_PATH=/api
# LITELLM_ENDPOINT=http://litellm_proxy:4000
# OPENAI_API_BASE_URL=https://your-domain.com/v1